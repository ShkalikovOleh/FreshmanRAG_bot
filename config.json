{
    "graph_type": "simple_rag",
    "db_connection": "postgresql+psycopg://{env:POSTGRES_USER}:{env:POSTGRES_PASSWORD}@{env.POSTGRES_HOST}:5432/{env.POSTGRES_DB}",
    "llm_config": {
        "llm_type": "gemma2_llama_cpp",
        "llm_args": {
            "model_path": ".models/model.gguf",
            "temperature": 0.75,
            "max_tokens": 2000,
            "top_p": 1,
            "use_mmap": false,
            "mlock": true,
            "n_ctx": 4096
        }
    },
    "retriever_config": {
        "vector_store_type": "pgvector",
        "vector_store_args": {
            "connection": "postgresql+psycopg://{env:POSTGRES_USER}:{env:POSTGRES_PASSWORD}@{env.POSTGRES_HOST}:5432/{env.POSTGRES_DB}",
            "async_mode": true
        },
        "embeeddings_type": "hugging_face",
        "embeeddings_args": {
            "model_name": ".models/ukr-paraphrase-multilingual-mpnet-base"
        }
    }
}